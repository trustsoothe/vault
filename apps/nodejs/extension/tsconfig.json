{
  "compilerOptions": {
    "strict": false,
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "moduleResolution": "node",
    "module": "es6",
    "esModuleInterop": true,
    "sourceMap": false,
    "rootDir": "src",
    "outDir": "dist/js",
    "jsx": "react",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "allowJs": true,
    "checkJs": false
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ]
}
